            <conn_id>4</conn_id>
            <td_url>tcp://gwf-md1.financial-trading-platform.com:41213</td_url>
            <md_url></md_url>
            <conn_type>4</conn_type>
            <investor_id>10101685</investor_id>
        </connection>
    </connections>
    <riskControler>
        <fullPrice>20</fullPrice>
        <stopLossPrice>5</stopLossPrice>
    </riskControler>
    <models>
