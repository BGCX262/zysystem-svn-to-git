<?xml version="1.0" encoding="utf-8"?>
<!--ZY System Server config\-->
<config>
    <server_id>4</server_id>
    <instruments>y1309</instruments>
    <!-- 日志配置 -->
    <log>
        <log_type>1</log_type>
        <!-- 日志路径1:屏幕输出，2文件输出 -->
        <log_path>./LOG/</log_path>
        <log_level>7</log_level>
